{
  "buildCommand": "npm run build",
  "out    }
  ]
}irectory": "dist",
  "functions": {
    "api/index.js": {
      "maxDuration": 30
    },
    "api/backend.js": {
      "maxDuration": 30
    }
  },
  "rewrites": [
    {
      "source": "/backend/(.*)",
      "destination": "/api/backend.js"
    },
    {
      "source": "/api/(.*)",
      "destination": "/api/index.js"
    },
    {
      "source": "/((?!api|backend|_next|assets|favicon).*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache, no-store, must-revalidate"
        }
      ]
    },
    {
      "source": "/backend/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache, no-store, must-revalidate"
        }
      ]
    }
  ]
}
