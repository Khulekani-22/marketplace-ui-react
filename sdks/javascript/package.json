{
  "name": "@marketplace/sdk",
  "version": "1.0.0",
  "description": "Official JavaScript/TypeScript SDK for Marketplace API",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "test": "jest",
    "prepublishOnly": "npm run build",
    "lint": "eslint src/**/*.ts",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "marketplace",
    "api",
    "sdk",
    "client",
    "typescript",
    "rest"
  ],
  "author": "Marketplace Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/marketplace-sdk-js.git"
  },
  "bugs": {
    "url": "https://github.com/your-org/marketplace-sdk-js/issues"
  },
  "homepage": "https://github.com/your-org/marketplace-sdk-js#readme",
  "dependencies": {
    "axios": "^1.6.0",
    "firebase": "^10.7.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "eslint": "^8.55.0",
    "jest": "^29.7.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
