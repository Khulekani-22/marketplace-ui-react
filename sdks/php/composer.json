{
  "name": "marketplace/sdk",
  "description": "Official PHP SDK for Marketplace API",
  "version": "1.0.0",
  "type": "library",
  "keywords": [
    "marketplace",
    "api",
    "sdk",
    "client",
    "rest"
  ],
  "homepage": "https://github.com/your-org/marketplace-sdk-php",
  "license": "MIT",
  "authors": [
    {
      "name": "Marketplace Team",
      "email": "support@your-api.com"
    }
  ],
  "require": {
    "php": ">=8.0",
    "guzzlehttp/guzzle": "^7.5",
    "ext-json": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0",
    "phpstan/phpstan": "^1.10"
  },
  "autoload": {
    "psr-4": {
      "Marketplace\\SDK\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Marketplace\\SDK\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "phpunit",
    "analyse": "phpstan analyse src --level=8"
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
