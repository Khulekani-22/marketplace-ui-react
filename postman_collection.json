        {
          "name": "List Services",
          "request": {
            "auth": {
              "type": "noauth"
            },
            "method": "GET",
            "header": [
              {
                "key": "X-Tenant-ID",
                "value": "{{tenant_id}}"
              }
            ],
            "url": {
              "raw": "{{base_url}}/data/services?page=1&pageSize=20",
              "host": ["{{base_url}}"],
              "path": ["data", "services"],
              "query": [
                {
                  "key": "page",
                  "value": "1"
                },
                {
                  "key": "pageSize",
                  "value": "20"
                },
                {
                  "key": "q",
                  "value": "",
                  "disabled": true
                },
                {
                  "key": "category",
                  "value": "",
                  "disabled": true
                },
                {
                  "key": "featured",
                  "value": "true",
                  "disabled": true
                },
                {
                  "key": "minPrice",
                  "value": "0",
                  "disabled": true
                },
                {
                  "key": "maxPrice",
                  "value": "1000",
                  "disabled": true
                }
              ]
            }
          }
        },
        {
          "name": "Get My Services",
          "request": {
            "method": "GET",
            "header": [
              {
                "key": "X-Tenant-ID",
                "value": "{{tenant_id}}"
              }
            ],
            "url": {
              "raw": "{{base_url}}/data/services/mine",
              "host": ["{{base_url}}"],
              "path": ["data", "services", "mine"],
              "query": [
                {
                  "key": "refresh",
                  "value": "true",
                  "disabled": true
                }
              ]
            }
          }
        },
        {
          "name": "Create Service",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "X-Tenant-ID",
                "value": "{{tenant_id}}"
              },
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"title\": \"New Service\",\n  \"description\": \"Service description\",\n  \"category\": \"Business\",\n  \"price\": 99.99,\n  \"currency\": \"ZAR\",\n  \"vendor\": \"Vendor Name\",\n  \"contactEmail\": \"vendor@example.com\",\n  \"featured\": false\n}"
            },
            "url": {
              "raw": "{{base_url}}/data/services",
              "host": ["{{base_url}}"],
              "path": ["data", "services"]
            }
          }
        },
        {
          "name": "Update Service",
          "request": {
            "method": "PUT",
            "header": [
              {
                "key": "X-Tenant-ID",
                "value": "{{tenant_id}}"
              },
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"title\": \"Updated Service\",\n  \"price\": 149.99\n}"
            },
            "url": {
              "raw": "{{base_url}}/data/services/:id",
              "host": ["{{base_url}}"],
              "path": ["data", "services", ":id"],
              "variable": [
                {
                  "key": "id",
                  "value": "service-id-here"
                }
              ]
            }
          }
        },
        {
          "name": "Delete Service",
          "request": {
            "method": "DELETE",
            "header": [
              {
                "key": "X-Tenant-ID",
                "value": "{{tenant_id}}"
              }
            ],
            "url": {
              "raw": "{{base_url}}/data/services/:id",
              "host": ["{{base_url}}"],
              "path": ["data", "services", ":id"],
              "variable": [
                {
                  "key": "id",
                  "value": "service-id-here"
                }
              ]
            }
          }
        },
        {
          "name": "Add Review",
          "request": {
            "auth": {
              "type": "noauth"
            },
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"rating\": 5,\n  \"comment\": \"Excellent service!\",\n  \"author\": \"John Doe\",\n  \"authorEmail\": \"john@example.com\"\n}"
            },
            "url": {
              "raw": "{{base_url}}/data/services/:id/reviews",
              "host": ["{{base_url}}"],
              "path": ["data", "services", ":id", "reviews"],
              "variable": [
                {
                  "key": "id",
                  "value": "service-id-here"
                }
              ]
            }
          }
        }
      ]
